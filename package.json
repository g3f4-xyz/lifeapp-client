{
  "name": "lifeapp-client",
  "version": "3.0.0",
  "homepage": "https://g3f4-xyz.github.io/lifeapp-client/",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production react-scripts build && sw-precache --config=sw-precache-config.js && cra-append-sw ./public/notifications-worker.js",
    "get:local:schema": "fetch-graphql-schema http://localhost:9000/graphql -o graphql.schema.json",
    "get:remote:schema": "fetch-graphql-schema https://lifeapp-server.herokuapp.com/graphql -o graphql.schema.json",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "relay": "relay-compiler --src ./src --schema ./graphql.schema.json --language typescript --noFutureProofEnums",
    "relay:watch": "relay-compiler --src ./src --schema ./graphql.schema.json --language typescript --noFutureProofEnums --watch",
    "relay:clear": "find . -name __generated__ -exec rm -rf {} \\;",
    "start": "react-scripts start",
    "start:remote": "REACT_APP_GRAPHQL_HOST=https://lifeapp-server.herokuapp.com/graphql react-scripts start",
    "test": "react-scripts test"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "dependencies": {
    "@auth0/auth0-react": "1.5.0",
    "@material-ui/core": "4.4.3",
    "@material-ui/icons": "4.4.3",
    "@types/classnames": "2.2.9",
    "@types/immutability-helper": "2.6.3",
    "@types/jest": "24.0.18",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/react-grid-layout": "0.16.7",
    "@types/react-input-mask": "2.0.3",
    "@types/react-router-dom": "5.1.8",
    "@types/relay-runtime": "11.0.1",
    "babel-plugin-relay": "11.0.2",
    "classnames": "2.3.1",
    "cra-append-sw": "2.6.1",
    "date-fns-timezone": "0.1.4",
    "graphql": "15.5.1",
    "history": "5.0.0",
    "immutability-helper": "3.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-grid-layout": "0.16.6",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "relay-compiler": "11.0.2",
    "relay-compiler-language-typescript": "14.0.0",
    "relay-hooks": "4.2.0",
    "sw-precache": "5.2.1",
    "typescript": "4.3.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-relay": "1.8.2",
    "fetch-graphql-schema": "^0.2.1",
    "prettier": "2.3.0",
    "relay-config": "11.0.2"
  }
}
