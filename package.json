{
  "name": "lifeapp-client",
  "version": "2.0.0",
  "dependencies": {
    "@material-ui/core": "3.9.1",
    "@material-ui/icons": "3.0.2",
    "@types/classnames": "2.2.7",
    "@types/express": "4.16.1",
    "@types/immutability-helper": "2.6.3",
    "@types/jest": "23.3.13",
    "@types/js-cookie": "2.2.0",
    "@types/react": "16.7.22",
    "@types/react-dom": "16.0.11",
    "@types/react-grid-layout": "0.16.6",
    "@types/react-input-mask": "2.0.1",
    "@types/react-jss": "8.6.1",
    "@types/react-relay": "1.3.10",
    "babel-plugin-relay": "2.0.0-rc.2",
    "classnames": "2.2.6",
    "fetch-graphql-schema": "0.2.1",
    "graphql": "14.1.1",
    "graphql-compiler": "^1.7.0",
    "immutability-helper": "3.0.0",
    "js-cookie": "2.2.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-grid-layout": "0.16.6",
    "react-input-mask": "2.0.4",
    "react-jss": "8.6.1",
    "react-relay": "2.0.0",
    "react-scripts": "2.1.3",
    "relay-compiler": "2.0.0",
    "relay-compiler-language-typescript": "2.0.0",
    "replace": "1.0.1",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "typescript": "^3.3.1"
  },
  "license": "MIT",
  "scripts": {
    "build:local": "REACT_APP_HOST=http://localhost:30000 react-scripts build && cp public/subscriptionWorker.js ./build/",
    "build:local:package": "yarn run build:local && tar -zcvf client-package-local-$npm_package_version.tar.gz build/",
    "build:heroku": "REACT_APP_GRAPHQL_HOST=https://lifeapp-server.herokuapp.com/graphql react-scripts build && cp public/subscriptionWorker.js ./build/",
    "build:heroku:package": "yarn run build:heroku && tar -zcvf client-package-heroku-$npm_package_version.tar.gz build/",
    "start": "REACT_APP_HOST=http://localhost:30000 react-scripts start",
    "start:remote": "REACT_APP_GRAPHQL_HOST=https://lifeapp-server.herokuapp.com/graphql react-scripts start",
    "run:local": "ts-node -P server/tsconfig.json server/index",
    "build": "react-scripts build",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --fix",
    "test": "react-scripts test",
    "get:local:schema": "fetch-graphql-schema http://localhost:30000/graphql -o graphql.schema.json",
    "get:remote:schema": "fetch-graphql-schema https://lifeapp-server.herokuapp.com/graphql -o graphql.schema.json",
    "relay": "relay-compiler --src ./src --schema ./graphql.schema.json --language typescript",
    "relay:watch": "relay-compiler --src ./src --schema ./graphql.schema.json --language typescript --watch",
    "add-tag": "git tag -a v$npm_package_version -m \"client version $npm_package_version\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {}
}
